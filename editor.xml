<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
    <option name="USE_LIGATURES" value="true" />
    <option name="SECONDARY_FONT_FAMILY" value="Fira Code Light" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc; *.ftl; *.html; *.htm; *.mjml; *.properties;*.js;" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
  </component>
  <component name="LiveEditOptions" chromeUpdateJSOnChanges="true" restartIfHotSwapFailed="true">
    <option name="elementHighlighting" value="true" />
  </component>
  <component name="SqlCodeCompletionSettings">
    <option name="customMappings">
      <map>
        <entry key="log" value="console.log(&quot;line,&quot; $1);" />
      </map>
    </option>
    <CUSTOM_ALIAS_MAPPINGS>
      <entry table-name="log" value="console.log(&quot;line,&quot; $1);" />
    </CUSTOM_ALIAS_MAPPINGS>
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
  </component>
</application>